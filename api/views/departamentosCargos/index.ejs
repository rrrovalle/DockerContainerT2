<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Teste page</title>
</head>

<body>
    <div class="container my-5">
        <h1>Tabela Departamentos Cargos</h1>
        <button class="btn btn-danger my-3" onclick="window.location.href='../trabalho_a2'">
            Voltar
        </button>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">id Departamento</th>
                    <th scope="col">id Cargo</th>
                </tr>
            </thead>
            <tbody>
                <% if (items) { items.forEach(i=> { %>
                    <tr>
                        <th scope="row">
                            <%= i.cd_departamento %>
                        </th>
                        <th scope="row">
                            <%= i.cd_cargo %>
                        </th>
                    </tr>
                    <% }) } %>
            </tbody>

        </table>

    </div>
</body>

</html>